        <?php
            
            $dizi =array("a","b","c");
            echo "<pre>";
            print_r($dizi);
            echo "</pre>";

            echo "<hr>";

            array_push($dizi,"d","e","f"); //dizeye  d ve e elemanını ekliyoruz.

            echo "<pre>";
            print_r($dizi);
            echo "</pre>";

            echo "<hr>";

            array_pop($dizi); // diziden son elemanı çıkartıyorum.

            echo "<pre>";
            print_r($dizi);
            echo "</pre>";

            

        ?>
    </div>
    
